   "source": [
    "def gradeStudents(grades):\n",
    "    for grade in grades:\n",
    "        if grade < 38:\n",
    "            print(grade)\n",
    "        elif (grade + 1) % 5 == 0:\n",
    "            print(grade + 1)\n",
    "        elif (grade + 2) % 5 == 0:\n",
    "            print(grade + 2)\n",
    "        else:\n",
    "            print(grade)\n",
    "\n",
    "\n",
    "gradeStudents([73, 67, 38, 33])"
