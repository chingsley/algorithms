    "def kangaroo(x1, v1, x2, v2):\n",
    "    if(x1 > x2 and v1 > v2):\n",
    "        return 'NO'\n",
    "    elif(x1 < x2 and v1 < v2):\n",
    "        return 'NO'\n",
    "    elif(x1 == x2 and v1 != v2):\n",
    "        return 'NO'\n",
    "    elif(x1 != x2 and v1 == v2):\n",
    "        return 'NO'\n",
    "    else:\n",
    "        if (x2 - x1) % (v1 - v2) == 0:\n",
    "            return 'YES'\n",
    "        else:\n",
    "            return 'NO'\n",
    "        \n",
    "print(kangaroo(1, 2, 2, 3))\n",
    "print(kangaroo(0, 3, 4, 2))"
