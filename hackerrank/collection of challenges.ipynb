    "def gradeStudents(grades):\n",
    "    for i in range(len(grades)):\n",
    "        if grades[i] >= 38 and (grades[i] + 1) % 5 == 0:\n",
    "            grades[i] = grades[i] + 1\n",
    "        elif grades[i] >= 38 and (grades[i] + 2) % 5 == 0:\n",
    "            grades[i] = grades[i] + 2\n",
    "    return grades\n",
    "\n",
    "\n",
