    "    elif(year > 1918 and ((year % 400 == 0) or (year % 4 == 0 and year % 100 != 0))):\n",
    "        return 29\n",
    "    elif(year == 1918):\n",
    "        return 15\n",
    "    else:\n",
    "        return 28\n",
