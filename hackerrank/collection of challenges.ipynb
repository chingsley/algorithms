    "def isKaprekar(num):\n",
    "    if(num == 1):\n",
    "        return True\n",
    "    elif(num == 2 or num == 3):\n",
    "        return False\n",
    "    else:\n",
    "        numlen = len(str(num))\n",
    "        sqrstr = str(num * num)\n",
    "        sqrlen = len(str(num * num))\n",
    "        diff = sqrlen-numlen\n",
    "        firstpart, secondpart = sqrstr[:diff], sqrstr[diff:]\n",
    "        return int(firstpart) + int(secondpart) == num\n",
    "\n",
    "def kaprekarNumbers(p, q):\n",
    "    result = []\n",
    "    for num in range(p, q+1):\n",
    "        if(isKaprekar(num)):\n",
    "            result.append(str(num))\n",
    "    \n",
    "    if(len(result) > 0):\n",
    "        print(' '.join(result))\n",
    "    else:\n",
    "        print('INVALID RANGE')\n",
    "\n",
    "\n",
    "kaprekarNumbers(55, 50)\n"
   ]
